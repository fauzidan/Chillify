        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Chillify">
        <activity android:name=".music_edit"></activity>
        <activity android:name=".music_ingfo" />
        <activity android:name=".music_insert" />
        <activity android:name=".user_dashboard" />
        <activity android:name=".user_register" />
        <activity android:name=".Dashboard" />
        <activity android:name=".MainActivity" />
        <activity android:name=".UserLogin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>